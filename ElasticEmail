echo "  $ops" | column -t -s ';'
    echo ""
    echo -e "${BOLD}${WHITE}[security]${OFF}"
read -r -d '' ops <<EOF
  ${CYAN}securityApikeysByNameDelete${OFF};Delete ApiKey (AUTH)
